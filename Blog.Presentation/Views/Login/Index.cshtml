@using Blog.Application.Models.DTOs;
@model LoginDTO

<form method="post">
<table>
    <tr>
        <td>User Email:</td>
        <td>
            <input type="email"  asp-for="UserName"/>
        </td>
    </tr>
        <tr>
            <td>Password</td>
            <td>
                <input type="password" asp-for="Password" />
            </td>
        </tr>
        <tr>
            <td colspan="2">
                <input type="submit" value="Login" />
            </td>
        </tr>
</table>
</form>
